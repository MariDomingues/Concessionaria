program Project1;

uses
  Vcl.Forms,
  UnitLogin in 'UnitLogin.pas' {Unt_Login},
  UnitDM in 'UnitDM.pas' {DM: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TUnt_Login, Unt_Login);
  Application.CreateForm(TDM, DM);
  Application.Run;
end.
